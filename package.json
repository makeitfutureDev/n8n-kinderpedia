{
  "name": "n8n-nodes-kinderpedia",
  "version": "0.1.0",
  "description": "n8n node for Kinderpedia API integration",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "kinderpedia"
  ],
  "license": "MIT",
  "homepage": "https://github.com/makeitfutureDev/n8n-kinderpedia#readme",
  "author": {
    "name": "Alex",
    "email": "apps@makeitfuture.eu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeitfutureDev/n8n-kinderpedia.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" package.json",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KinderpediaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Kinderpedia/Kinderpedia.node.js",
      "dist/nodes/Kinderpedia/KinderpediaTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
